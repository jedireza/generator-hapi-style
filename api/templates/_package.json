{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "lab -c -L",
    "test-cover": "lab -c -r html -o ./test/artifacts/coverage.html && open ./test/artifacts/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "<%= gitRepo %>"
  },
  "author": "<%= author %><% if (authorEmail) { %> <<%= authorEmail %>><% } %>",
  "license": "<%= license %>",
  "bugs": {
    "url": "<%= bugsUrl %>"
  },
  "homepage": "<%= homepageUrl %>",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "confidence": "^1.x.x",
    "glue": "^2.x.x",
    "hapi": "^11.x.x",
    "hoek": "^3.x.x"
  },
  "devDependencies": {
    "code": "^2.x.x",
    "lab": "^7.x.x",
    "nodemon": "^1.x.x"
  }
}
