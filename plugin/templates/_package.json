  "bugs": {
    "url": "<%= bugsUrl %>"
  },
  "homepage": "<%= homepageUrl %>",
  "devDependencies": {
    "hapi": "^8.x.x",
    "code": "^1.x.x",
    "lab": "^5.x.x"
  }
}
